<section class="education section">
    {{#with myData.education}}
    <h2 class="section-title">My Education</h2>

    <div class="education__container bd-grid">
        <div class="education__content">
            <div>
                <h3 class="education__year">2001 - 2014</h3>
                <span class="education__school">โรงเรียนเทศบาลวัดดอนไก่ดี</span>
            </div>

            <div class="education__time">
                <span class="education__rounder"></span>
                <span class="education__line"></span>
            </div>

            <div>
                <h3 class="education__race">{{โรงเรียนเทศบาลวัดดอนไก่ดี}}</h3>
                <span class="education__specialty">basic</span>
            </div>
        </div>

        <div class="education__content">
            <div>
                <h3 class="education__year">2015 - 2018</h3>
                <span class="education__school">โรงเรียนกระทุ่มแบนวิเศษสมุทคุณ</span>
            </div>

            <div class="education__time">
                <span class="education__rounder"></span>
                <span class="education__line"></span>
            </div>

            <div>
                <h3 class="education__race">{{โรงเรียนกระทุ่มแบนวิเศษสมุทคุณ}}</h3>
                <span class="education__specialty">Normal</span>
            </div>
        </div>

        <div class="education__content">
            <div>
                <h3 class="education__year">2019-2020</h3>
                <span class="education__school">มหาวิทยาลัยราชภัฎนครปฐม</span>
            </div>

            <div class="education__time">
                <span class="education__rounder"></span>
                <span class="education__line"></span>
            </div>

            <div>
                <h3 class="education__race">{{มหาวิทยาลัยราชภัฎนครปฐม}}</h3>
                <span class="education__specialty">Advanc</span>
            </div>
        </div>
    </div> 
    {{/with}}
</section>

<footer class="footer section">
    <div class="footer__container bd-grid">
        <h1 class="footer__title">Mhapao</h1>
